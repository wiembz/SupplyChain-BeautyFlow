{
  "name": "Top5ProduitsVendus",
  "expression": [
    "",
    "VAR TopProduits = ",
    "    TOPN(",
    "        5,",
    "        SUMMARIZE(",
    "            'public Fact_Production',",
    "            'public Fact_Production'[Fk_Products],",
    "            \"QuantitéTotale\", SUM('public Fact_Production'[QuantityInventory])",
    "        ),",
    "        [QuantitéTotale], DESC",
    "    )",
    "VAR TableAvecNoms = ",
    "    ADDCOLUMNS(",
    "        TopProduits,",
    "        \"NomProduit\",",
    "        LOOKUPVALUE(",
    "            'public DimProducts'[ProductName],",
    "            'public DimProducts'[Pk_Products],",
    "            'public Fact_Production'[Fk_Products]",
    "        )",
    "    )",
    "VAR ResultatTexte =",
    "    CONCATENATEX(",
    "        TableAvecNoms,",
    "        [NomProduit] & \" (\" & [QuantitéTotale] & \")\",",
    "        \", \",",
    "        [QuantitéTotale],",
    "        DESC",
    "    )",
    "RETURN",
    "    ResultatTexte",
    ""
  ],
  "lineageTag": "b1a7c545-a0e0-43b2-923d-1b10f416cd22"
}